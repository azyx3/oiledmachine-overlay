# @FUNCTION: _resolve_hotfix_default
# @DESCRIPTION:
# Applies the fix or warns if not applied
# @CODE
# $1 - overrides patch of copy patch, intended to fix patches (optional)
# @CODE
function _resolve_hotfix_default() {
	local cve_dupe="${CVE_ID_}DUPE_FIX_CVE"
	local parts="${CVE_ID_}PARTS"
	local fn
	if [[ -z "${1}" ]] ; then
		fn="${DISTDIR}/${!cve_fn}"
	else
		fn="${1}"
	fi
	if ! approve_cve_fix \
		|| blacklist_cve_fix \
		|| ! approve_cve_accept_disputed ; then
		einfo \
"${CVE_ID} fix rejected by user.  A fix will not be applied.\n\
This is a ${!cve_severity} risk vulnerability."
		return 0
	fi
	if use cve_hotfix ; then
		if [[ -n "${!cve_dupe}" ]] ; then
			einfo \
"Fix applied in ${!cve_dupe}.  Skipping..."
		elif [ -e "${fn}" ] ; then
			einfo \
"Resolving ${CVE_ID}.  ${!cve_fn} may break in different kernel versions."
			if [[ -z "${!parts}" ]] ; then
				_dpatch "${PATCH_OPS}" "${fn}"
				return $?
			else
				local ret=0
				for i in $(seq 1 ${!parts}) ; do
					fn="${CVE_ID_}FN_PART${i}"
					_dpatch "${PATCH_OPS}" "${!fn}"
					ret=$((ret | $?))
				done
				return ${ret}
			fi
		else
			ewarn \
"No ${CVE_ID} fixes applied.  This is a ${!cve_severity} risk vulnerability."
		fi
	else
		ewarn \
"No ${CVE_ID} fixes applied.  This is a ${!cve_severity} risk vulnerability."
	fi
	return 0
}

# @FUNCTION: _resolve_hotfix_default_filesdir
# @DESCRIPTION:
# Applies the fix or warns if not applied
function _resolve_hotfix_default_filesdir() {
	if ! approve_cve_filesdir \
		|| blacklist_cve_fix \
		|| ! approve_cve_accept_disputed ; then
		einfo \
"${CVE_ID} fix rejected by user.  A fix will not be applied.\n\
This is a ${!cve_severity} risk vulnerability."
		return 0
	fi
	if use cve_hotfix ; then
		if [ -e "${FILESDIR}/${!cve_fn_filesdir}" ] ; then
			einfo \
"Resolving ${CVE_ID}.  ${!cve_fn} may break in different kernel versions."
			_dpatch "${PATCH_OPS}" "${FILESDIR}/${!cve_fn_filesdir}"
			return $?
		else
			ewarn \
"No ${CVE_ID} fixes applied.  This is a ${!cve_severity} risk vulnerability."
		fi
	else
		ewarn \
"No ${CVE_ID} fixes applied.  This is a ${!cve_severity} risk vulnerability."
	fi
	return 0
}

