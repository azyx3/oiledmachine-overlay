# @FUNCTION: approve_cve_fix
# @DESCRIPTION:
# Allows user to approve CVE fixes based on trust and quality.
# User will define CVE_FIX_TRUST_LEVEL or the default will be used.
# @RETURNS: 0-approved, 1-not approved
function approve_cve_fix() {
	local cve_trust="${CVE_ID_}TRUST"
	if ((${!cve_trust} & ${CVE_FIX_TRUST_LEVEL})) ; then
		return 0
	fi
	return 1
}

# @FUNCTION: approve_cve_filesdir
# @DESCRIPTION:
# Checks if CVE_ALLOW_EBUILD_MAINTAINER_FILESDIR backports are accepted by admin
# @RETURNS: 0-approved, 1-not approved
function approve_cve_filesdir() {
	local cve_trust="${CVE_ID_}TRUST"
	if (( (${!cve_trust} & ${CVE_ALLOW_EBUILD_MAINTAINER_FILESDIR}) \
		&& (${CVE_ALLOW_EBUILD_MAINTAINER_FILESDIR} & ${CVE_FIX_TRUST_LEVEL}) ))
	then
		return 0
	fi
	return 1
}

# @FUNCTION: approve_cve_addendum
# @DESCRIPTION:
# Checks if CVE_ALLOW_EBUILD_MAINTAINER_ADDENDUM_CLASS_A are accepted by admin
# @RETURNS: 0-approved, 1-not approved
function approve_cve_addendum() {
	local cve_trust="${CVE_ID_}TRUST"
	if (( (${!cve_trust} & ${CVE_ALLOW_EBUILD_MAINTAINER_ADDENDUM_CLASS_A}) \
		&& (${CVE_ALLOW_EBUILD_MAINTAINER_ADDENDUM_CLASS_A} & ${CVE_FIX_TRUST_LEVEL}) ))
	then
		return 0
	fi
	return 1
}

# @FUNCTION: approve_cve_accept_disputed
# @DESCRIPTION:
# Checks if CVE_FIX_REJECT_DISPUTED is set to 1 and rejects fixes
# @RETURNS: 0-approved so fix it, 1-unapproved so reject
function approve_cve_accept_disputed() {
	local cve_disputed="${CVE_ID_}DISPUTED"
	if [[ -z "${!cve_disputed}" ]] ; then
		return 0
	elif [[ -n "${!cve_disputed}" \
		&& "${!cve_disputed}" == "1" \
		&& "${CVE_FIX_REJECT_DISPUTED}" == "0" ]] ; then
		return 0
	else
		return 1
	fi
}

# @FUNCTION: black_cve_fix
# @DESCRIPTION:
# Allows user to blacklist CVE fixes for reasons like stability or
# data corruption, etc.
#
# For example:
# CVE_BLACKLIST_FIXES="CVE-2019-19039 CVE-2019-19252"
#
# Should be added to make.conf or per-package env.
#
# @RETURNS: 0 on reject cve fix, 1 on accept cve fix
function blacklist_cve_fix() {
	if [[ -n "${CVE_BLACKLIST_FIXES}" ]] ; then
		for c in ${CVE_BLACKLIST_FIXES} ; do
			if [[ "${CVE_ID}" == "${c}" ]] ; then
				return 0
			fi
		done
	fi
	return 1
}

# @FUNCTION: _fetch_cve_boilerplate_msg
# @DESCRIPTION:
# Message to report the important items to user about the CVE.
function _fetch_cve_boilerplate_msg() {
	local CVE_ID_="${CVE_ID//-/_}_"
	local cve_severity="${CVE_ID_}SEVERITY"
	local PS="${CVE_ID_}FIX_SRC_URI"
	local NIST_NVD_CVE_M="https://nvd.nist.gov/vuln/detail/${CVE_ID}"
	local MITRE_M="https://cve.mitre.org/cgi-bin/cvename.cgi?name=${CVE_ID}"
	local summary="${CVE_ID_}SUMMARY"
	local parts="${CVE_ID_}PARTS"
	if [[ -z "${!parts}" ]] ; then
		local pm="${CVE_ID_}PM"
		ewarn
		ewarn \
"${CVE_ID}\n\
Severity: ${!cve_severity}\n\
Synopsis: ${!summary}\n\
NIST NVD URI: ${NIST_NVD_CVE_M}\n\
MITRE URI: ${MITRE_M}\n\
Patch download: ${!PS}\n\
Patch message: ${!pm}"
		ewarn
	else
		local downloads=""
		local messages=""
		for i in $(seq 1 ${!parts}) ; do
			local download="${CVE_ID_}FIX_SRC_URI_PART${i}"
			local message="${CVE_ID_}PM_PART${i}"
			downloads+="${!download} "
			messages+="${!message} "
		done
		ewarn
		ewarn \
"${CVE_ID}\n\
Severity: ${!cve_severity}\n\
Synopsis: ${!summary}\n\
NIST NVD URI: ${NIST_NVD_CVE_M}\n\
MITRE URI: ${MITRE_M}\n\
Patch downloads: ${downloads}\n\
Patch messages: ${messages}"
		ewarn
	fi
}

# @FUNCTION: _fetch_cve_boilerplate_msg_footer
# @DESCRIPTION:
# Message to report action to user to fix the CVE.
function _fetch_cve_boilerplate_msg_footer() {
	local CVE_ID_="${CVE_ID//-/_}_"
	local cve_fn="${CVE_ID_}FN"
	local pm="${CVE_ID_}PM"
	local cve_dupe="${CVE_ID_}DUPE_FIX_CVE"
	local mpm1="${CVE_ID_}PM_PART1"

	if [[ -n "${!cve_dupe}" ]] ; then
		einfo \
"Dupe fix found in ${!cve_dupe}.  Skipping..."
	elif [[ -z "${!cve_fn}" ]] \
		|| [[ -z "${!parts}" && -z "${!pm}" ]] \
		|| [[ -n "${!parts}" && -z "${!mpm1}" ]] ; then
		eerror \
"No de-facto patch exists for ${CVE_ID} or the patch is undergoing code review.  No patch\n\
will be applied.  This fix is still being worked on."
	elif use cve_hotfix ; then
		einfo "A ${CVE_ID} fix will be applied."
	else
		ewarn \
"Re-enable the cve_hotfix USE flag to fix this, or you may ignore this and\n\
wait for an official fix in later kernel point releases."
		ewarn
		echo -e "\07" # ring the bell
		[[ "${CVE_DELAY}" == "1" ]] && sleep 30s
	fi
	return 0
}

# @FUNCTION: fetch_cve_2007_3732_hotfix
# @DESCRIPTION:
# Checks for the CVE_2007_3732 patch
function fetch_cve_2007_3732_hotfix() {
	local CVE_ID="CVE-2007-3732"
	if ver_test ${PV} -ge 2.6.23 ; then
		einfo "Skipping obsolete ${CVE_ID}"
		return 0
	fi
	if ver_test ${PV} -lt 2.6 ; then
		einfo "Skipping versions below not in advisory ${CVE_ID}"
		return 0
	fi
	if grep -F -e \
		"#define DO_ERROR_INFO(trapnr, signr, str, name, sicode, siaddr, irq)" \
		"${S}/arch/i386/kernel/traps.c" \
		>/dev/null
	then
		einfo "${CVE_ID} already patched."
		return 0
	fi
	_fetch_cve_boilerplate_msg
	_fetch_cve_boilerplate_msg_footer
	return 0
}

