			|| die
		einfo "Auto backporting ${CVE_ID} fix to ${PV}"
		sed -i -e \
		  "s|crypto/crypto_user_base.c|crypto/crypto_user.c|g" \
			"${T}/${PV}-${!cve_fn}" \
			|| die
		_resolve_hotfix_default "${T}/${PV}-${!cve_fn}"
	else
		_resolve_hotfix_default
	fi
	return $?
}

